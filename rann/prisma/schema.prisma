enum PsyTestDimensionQuestions {
  ATTITUDE
  DO
  KNOW
}

enum PsyTestCompetencyQuestions {
  VISION_OF_THE_FUTURE
  SOCIAL_SENSITIVITY
  LEADERSHIP
  DECISION_MAKING
  ACHIEVEMENT_ORIENTATION
  ACTION_INITIATIVE
  NETWORK_BUILDING
  CREATIVITY
  ADAPTATION_TO_CHANGE
}

model PsyTestQuestions {
  id                   String                       @id @default(dbgenerated("gen_random_uuid()"))
  created_at           DateTime                     @default(now()) @db.Timestamptz(6)
  updated_at           DateTime                     @db.Timestamptz(6)
  number               Int
  question             String
  dimension            PsyTestDimensionQuestions
  competency           PsyTestCompetencyQuestions[]
  correct_answer       Boolean
  exam_id              String                       @db.Uuid
  exam                 PsyTestExam                  @relation(fields: [exam_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  PsyTestSingleAnswers PsyTestSingleAnswers[]
}

enum PsyTestAnswersStatus {
  PENDING
  INPROGRESS
  FINALIZED
}

model PsyTestAnswers {
  id                         String                  @id @default(dbgenerated("gen_random_uuid()"))
  created_at                 DateTime                @default(now()) @db.Timestamptz(6)
  updated_at                 DateTime                @db.Timestamptz(6)
  examName                   String
  status                     PsyTestAnswersStatus
  singleAnswers              PsyTestSingleAnswers[]
  psyTestGroupAssignation_id String                  @unique @db.Uuid
  PsyTestGroupAssignation    PsyTestGroupAssignation @relation(fields: [psyTestGroupAssignation_id], references: [id])
  results                    Json?                   @db.JsonB
}

model PsyTestSingleAnswers {
  id                  String           @id @default(dbgenerated("gen_random_uuid()"))
  PsyTestQuestions_id String
  PsyTestQuestion     PsyTestQuestions @relation(fields: [PsyTestQuestions_id], references: [id])
  question            String
  answer              Boolean?
  psyTestAnswersId    String
  psyTestAnswers      PsyTestAnswers   @relation(fields: [psyTestAnswersId], references: [id])
  created_at          DateTime         @default(now()) @db.Timestamptz(6)
  updated_at          DateTime         @db.Timestamptz(6)
}
