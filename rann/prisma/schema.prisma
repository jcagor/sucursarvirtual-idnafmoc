enum PsyTestGroupStatus {
  PENDING
  INPROGRESS
  FINALIZED
}

enum PsyTestGroupAssignationStatus {
  PENDING
  INPROGRESS
  FINALIZED
  RETAKE
}

model PsyTestExam {
  id                      String                    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                    String
  created_at              DateTime                  @db.Timestamptz(6)
  updated_at              DateTime                  @db.Timestamptz(6)
  state                   Boolean?
  PsyTestGroupAssignation PsyTestGroupAssignation[]
  PsyTestQuestions        PsyTestQuestions[]
}

model PsyTestGroup {
  id                      String                    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  sequence                Int                       @default(autoincrement())
  name                    String
  number_persons          Decimal                   @default(0) @db.Decimal
  file_name               String
  md5_sum                 String
  current_status          PsyTestGroupStatus        @default(PENDING)
  crea_course_id          String?                   @db.Uuid
  crea_entrepreneurship   Boolean?
  created_at              DateTime                  @default(now()) @db.Timestamptz(6)
  updated_at              DateTime                  @db.Timestamptz(6)
  state                   Boolean?
  PsyTestGroupAssignation PsyTestGroupAssignation[]
  courseSchedule          courseSchedule?           @relation(fields: [crea_course_id], references: [id])
}

model PsyTestGroupAssignation {
  id                String                        @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  PsyTestPerson_id  String                        @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  PsyTestGroup_id   String                        @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  PsyTestExam_id    String                        @db.Uuid
  PsyTestAnswers_id String?                       @db.Uuid
  PsyTestExam       PsyTestExam                   @relation(fields: [PsyTestExam_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  PsyTestGroup      PsyTestGroup                  @relation(fields: [PsyTestGroup_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  PsyTestPerson     PsyTestPerson                 @relation(fields: [PsyTestPerson_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  PsyTestAnswers    PsyTestAnswers?
  state             Boolean?
  current_status    PsyTestGroupAssignationStatus @default(PENDING)
  created_at        DateTime                      @default(now()) @db.Timestamptz(6)
  updated_at        DateTime                      @default(now()) @db.Timestamptz(6)
  retake_id         String?                       @db.Uuid
  original          PsyTestGroupAssignation?      @relation("ExamReassignation", fields: [retake_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  retake            PsyTestGroupAssignation[]     @relation("ExamReassignation")
  retake_reason     String?
}

model PsyTestPerson {
  id                      String                    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  document_type           String                    @default("CC")
  document_number         String
  name                    String
  last_name               String?
  email                   String
  created_at              DateTime                  @default(now()) @db.Timestamptz(6)
  updated_at              DateTime                  @db.Timestamptz(6)
  state                   Boolean?
  PsyTestGroupAssignation PsyTestGroupAssignation[]
}

