}

model programSession {
  id                 String          @id @default(uuid()) @db.Uuid
  programSchedule    programSchedule @relation(fields: [programSchedule_id], references: [id])
  programSchedule_id String          @db.Uuid
  name               String
  typeSession        String
  endDate            DateTime
  value              String
  createdAt          DateTime        @default(now())
  updatedAt          DateTime        @updatedAt
}

model CodeCIIU {
  id                      String   @id @default(uuid()) @db.Uuid
  code                    String
