generator client {
  provider        = "prisma-client-js"
  previewFeatures = []
  binaryTargets   = ["native", "linux-musl", "linux-musl-openssl-3.0.x", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_GAMMA_URL")
}

model Document_Type {
  id                  String               @id @default(uuid()) @db.Uuid
  name                String
  createdAt           DateTime             @default(now())
  updatedAt           DateTime             @updatedAt
  businessProfiles    BusinessProfile[]
  businessValidations BusinessValidation[]
  selfManagement      SelfManagement[]
  coursePostulation   CoursePostulation[]
}

model BusinessProfile {
  id                  String                                  @id @default(uuid()) @db.Uuid
  document_type_id    String                                  @db.Uuid
  document_number     String                      
  data                Json                      
  createdAt           DateTime                                @default(now())
  updatedAt           DateTime                                @updatedAt
  appointments        Appointments[]                      
  workPlan            WorkPlan[]                      
  growthPlan          GrowthPlan[]                      
  courseSchedule      courseSchedule[]                        @relation("CourseScheduleEligibleBusiness")
  document_type       Document_Type                           @relation(fields: [document_type_id], references: [id])
  userProfile         UserProfile[]                           @relation("UserProfileBusiness")
  Programs            programSchedule[]                       @relation("BusinessProgramInscription")
  adminAssignHistory  BusinessAdminUserAssignationHistory[]
}

model BusinessValidation {
  id               String                   @id @default(uuid()) @db.Uuid
  document_type_id String                   @db.Uuid
  document_type    Document_Type            @relation(fields: [document_type_id], references: [id])
  document_number  String
  reason           String[]
  status           BusinessValidationStatus
  validatedAt      DateTime?                @db.Date
  createdAt        DateTime                 @default(now())
  updatedAt        DateTime                 @updatedAt
}

enum BusinessValidationStatus {
  APPROVED
  REJECTED
}

model Department {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model City {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Sector {
  id        String      @id @default(uuid()) @db.Uuid
  name      String
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
  subsector Subsector[]
  CodeCIIU  CodeCIIU[]
}

model Subsector {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  sector_id String   @db.Uuid
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  sector    Sector   @relation(fields: [sector_id], references: [id])
}

model SelfManagement {
  id               String        @id @default(uuid()) @db.Uuid
  document_type_id String        @db.Uuid
  document_number  String
  answers          Json
  analysis         Json
  createdAt        DateTime      @default(now())
  updatedAt        DateTime      @updatedAt
  document_type    Document_Type @relation(fields: [document_type_id], references: [id])
}

model Appointments {
  id                   String                 @id @default(uuid()) @db.Uuid
  businessProfile_id   String                 @db.Uuid
  date                 DateTime
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  attendance           Attendance
  businessProfile      BusinessProfile        @relation(fields: [businessProfile_id], references: [id])
  TechAssistanceRecord TechAssistanceRecord[]
  type                 AppointmentType        @default(CONSULT)
  appointmentHistory   AppointmentHistory[]
}

model AppointmentHistory {
  id            String       @id @default(uuid()) @db.Uuid
  appointmentId String       @db.Uuid
  oldDate       DateTime
  newDate       DateTime
  changedAt     DateTime     @default(now())
  appointment   Appointments @relation(fields: [appointmentId], references: [id])
}

model course {
  id              String           @id @default(uuid()) @db.Uuid
  name            String
  description     String
  iconUrl         String
  courseSchedules courseSchedule[]
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
}

enum CourseState {
  Created
  Cancelled
  Active
}

enum CourseAccess {
  OPEN
  CLOSED
}

model courseSchedule {
  id                                String                              @id @default(uuid()) @db.Uuid
  course                            course                              @relation(fields: [course_id], references: [id])
  course_id                         String                              @db.Uuid
  name                              String            
  startDate                         DateTime            
  endDate                           DateTime            
  modality                          String            
  typeUser                          String            
  sessions                          Int           
  supplier                          String            
  id_regional                       String            
  cost                              Int           
  state                             CourseState                         @default(Created)
  accessType                        CourseAccess                        @default(OPEN)
  description                       String            
  courseCurriculumMesh              CourseCurriculumMesh[]            
  coursePostulation                 CoursePostulation[]           
  courseRegistration                CourseRegistration[]            
  courseSession                     CourseSession[]           
  trainingCourses                   TrainingCourseSchedule[]            
  psyTestGroup                      PsyTestGroup[]            
  courseInscriptionAttemptHistory   CourseInscriptionAttemptHistory[]
  creaCourseRegisterHistory         CreaCourseRegisterHistory[]
  businessProfile                   BusinessProfile[]                   @relation("CourseScheduleEligibleBusiness")
  createdAt                         DateTime                            @default(now())
  updatedAt                         DateTime                            @updatedAt
}

model CourseCurriculumMesh {
  id                String         @id @default(uuid()) @db.Uuid
  courseSchedule    courseSchedule @relation(fields: [courseSchedule_id], references: [id])
  courseSchedule_id String         @db.Uuid
  name              String
  date              DateTime
  schedule          String
  sessionType       String
  session           String
}

model CourseSession {
  id                String         @id @default(uuid()) @db.Uuid
  courseSchedule    courseSchedule @relation(fields: [courseSchedule_id], references: [id])
  courseSchedule_id String         @db.Uuid
  name              String
  typeSession       String
  startTime         String
  endTime           String
  date              DateTime
}

model CoursePostulation {
  id                 String               @id @default(uuid()) @db.Uuid
  courseSchedule     courseSchedule       @relation(fields: [courseSchedule_id], references: [id])
  courseSchedule_id  String               @db.Uuid
  document_number    String
  document_type_id   String               @db.Uuid
  document_type      Document_Type        @relation(fields: [document_type_id], references: [id])
  courseRegistration CourseRegistration[]
  createdAt          DateTime             @default(now())
  updatedAt          DateTime             @updatedAt
}

model CourseRegistration {
  id                   String             @id @default(uuid()) @db.Uuid
  courseSchedule       courseSchedule     @relation(fields: [courseSchedule_id], references: [id])
  courseSchedule_id    String             @db.Uuid
  coursePostulation_id String?            @db.Uuid
  coursePostulation    CoursePostulation? @relation(fields: [coursePostulation_id], references: [id])
  document_type        String
  document_number      String
  firstName            String
  middleName           String
  firstLastName        String
  middleLastName       String
  phoneNumber          String
  email                String
  gender               String
  createdAt            DateTime           @default(now())
  updatedAt            DateTime           @updatedAt
}

model MonthlyReport {
  id               String   @id @default(uuid()) @db.Uuid
  document_type_id String   @db.Uuid
